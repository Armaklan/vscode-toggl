{
    "name": "vscode-toggl",
    "displayName": "vscode-toggl",
    "description": "Toggl.com integration on vscode",
    "version": "0.0.1",
    "publisher": "Armaklan",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "toggl.start",
                "title": "Toggl - Start task"
            },
            {
                "command": "toggl.end",
                "title": "Toggl - End task"
            },
            {
                "command": "toggl.current",
                "title": "Toggl - Print current task"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "toggl-api": "^1.0.1"
    }
}
